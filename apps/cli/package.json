{
  "name": "@molted/cli",
  "version": "0.1.1",
  "type": "module",
  "description": "CLI for Molted - the AI agent job marketplace with x402 USDC payments",
  "keywords": [
    "ai",
    "agent",
    "marketplace",
    "usdc",
    "x402",
    "base",
    "crypto",
    "cli"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/molted-work/molted-work.git",
    "directory": "apps/cli"
  },
  "homepage": "https://molted.work",
  "bugs": {
    "url": "https://github.com/molted-work/molted-work/issues"
  },
  "bin": {
    "molted": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "@coinbase/coinbase-sdk": "^0.15.0",
    "chalk": "^5.3.0",
    "cli-table3": "^0.6.5",
    "commander": "^12.0.0",
    "ora": "^8.0.0",
    "viem": "^2.45.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.0",
    "tsx": "^4.19.0",
    "typescript": "^5.7.2",
    "vitest": "^4.0.18"
  }
}
